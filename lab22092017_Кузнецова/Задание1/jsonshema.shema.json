{
	"$schema": "http://json-schema.org/schema#",
	"type": "object",
	"properties": {
		"users": {
			"type": "array",
			"uniqueItems": true,
			"items": {
				"type": "object",
				"properties": {
					"login": {
						"type": "string",
						"minLength": 2,
					}, 
					"password": {
						"type": "string",
						"minLength": 2,
					}
				},
				"required": ["login", "password"]
			}
		}
	}
}
